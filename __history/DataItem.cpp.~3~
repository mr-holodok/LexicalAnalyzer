//---------------------------------------------------------------------------

#pragma hdrstop

#include "DataItem.h"
//---------------------------------------------------------------------------
#pragma package(smart_init)

__fastcall DataItem::DataItem(long k, std::string v)
{
	this.key = k;
	this.value = v;
}

long DataItem::getKey()
{
	return this.key;
}

std::string DataItem::getValue()
{
	return this.value;
}
